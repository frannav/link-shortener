{
  "targetDefaults": {
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": true
    },
    "lint:fix": {
      "dependsOn": [
        "^lint:fix"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "cache": true
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
